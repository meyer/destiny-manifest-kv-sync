{
  "name": "destiny-manifest-cloudflare-worker-kv",
  "version": "0.0.0",
  "description": "GitHub action that syncs the latest Destiny 2 manifest to a specific Cloudflare Workers KV store",
  "author": "meyer",
  "private": true,
  "scripts": {
    "download-manifest-data": "rm -rf bulk-data && node scripts/download-manifest-data.mjs",
    "download-manifest-data-local": "rm -rf bulk-data && node -r dotenv/config scripts/download-manifest-data.mjs",
    "sync-kv": "./scripts/sync-kv.sh"
  },
  "dependencies": {
    "@actions/cache": "^3.2.1",
    "@actions/core": "^1.10.0",
    "@actions/exec": "^1.1.1",
    "@actions/github": "^5.1.1",
    "@actions/glob": "^0.4.0",
    "@actions/io": "^1.1.2",
    "@octokit/rest": "^19.0.7",
    "bungie-api-ts": "^4.22.2",
    "dotenv": "^16.0.3",
    "wrangler": "^2.12.2"
  },
  "devDependencies": {
    "@types/node": "^18"
  }
}
